{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "analyze": "ANALYZE=true pnpm build",
    "build": "next build",
    "build:registry": "tsx --tsconfig ./scripts/tsconfig.scripts.json scripts/build-registry.mts",
    "build:tw": "pnpm tailwindcss -i ./src/app/globals.css -o ./public/css/tailwind.css --minify",
    "db:deploy": "pnpm with-env prisma migrate deploy",
    "db:generate": "pnpm with-env prisma generate",
    "db:migrate:squash": "rm -rf prisma/migrations && pnpm with-env prisma migrate dev --name init",
    "db:push": "pnpm with-env prisma db push --skip-generate",
    "db:reset": "pnpm with-env prisma migrate reset --force && pnpm with-env prisma db push --accept-data-loss",
    "db:script": "pnpm with-env tsx prisma/scripts/script.ts",
    "db:seed": "pnpm with-env prisma db seed",
    "db:studio": "pnpm with-env prisma studio",
    "dev": "run-p dev:*",
    "dev:app": "next dev --turbo",
    "dev:db": "docker-compose up",
    "docker": "docker-compose -f docker-compose.web.yml --env-file .env.local up --build",
    "postinstall": "pnpm db:generate",
    "lint": "eslint . && prettier --check . --log-level warn",
    "lint:fix": "eslint . --fix && prettier --write . --log-level warn",
    "migrate": "pnpm with-env prisma migrate dev",
    "nuke": "rimraf '**/node_modules'",
    "scheduled": "node tooling/scripts/clearFiles.js",
    "start": "next start --port 3000",
    "sync:plate": "node tooling/scripts/sync-plate.cjs",
    "sync:potion-ui": "node tooling/scripts/sync-potion-ui.cjs",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch",
    "vendor": "sh tooling/scripts/vendor.sh",
    "with-env": "dotenv -e .env.local --"
  },
